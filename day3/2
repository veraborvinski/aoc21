#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int main(){

	 
    int number;
    int gamma[12];
    gamma[0] = 0;
    int a,b,t;
    for (int i = 1; i < 13; ++i)
    {
        printf("run %d\n", i);
        FILE *fp = fopen( "input.txt", "r" );
        if (fp!=NULL) 
        {
            char line[14];
            while(fgets(line, 14, fp)!= NULL)
            {
                
                    t=0;
                    for (int j = 0; j < i; ++j)
                    {
                        if (line[j]==(gamma[j]+48))
                        {
                            t++;
                        }
                    }
                    if(line[i] == 48 && t == i)
                    {
                        printf("%s\n", line);
                        a++;
                    }
                    else if(line[i] == 49 && t == i)
                    {
                        printf("%s\n", line);
                        b++;
                    }
                
                
            }

            if(a>b)
            {
                gamma[i] = 1;
            }
            else if (b>a)
            {
                gamma[i] = 0;
            }
            else if(a==b)
            {
                gamma[i] = 0;
            }
            a=0;
            b=0;
            
        }
        fclose (fp); 
    }
	return 0;
}
